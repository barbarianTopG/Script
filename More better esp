local TEAM_COLORS = {
    Enemy = Color3.fromRGB(255, 0, 0),
    Friendly = Color3.fromRGB(0, 255, 0),
    Chams = Color3.fromRGB(255, 0, 255),
    Visible = Color3.fromRGB(255, 255, 0),
    Invisible = Color3.fromRGB(255, 0, 0),
    Armor = Color3.fromRGB(0, 150, 255),
    Headshot = Color3.fromRGB(255, 100, 0),
    Critical = Color3.fromRGB(255, 0, 127),
    AI = Color3.fromRGB(255, 165, 0),
    Alert = Color3.fromRGB(255, 0, 0),
    Loot = Color3.fromRGB(0, 255, 255),
    Vehicle = Color3.fromRGB(150, 75, 0),
    VIP = Color3.fromRGB(255, 215, 0),
    Admin = Color3.fromRGB(75, 0, 130)
}

-- Configuration
local Config = {
    Enabled = true,
    ShowName = true,
    ShowHealth = true,
    ShowDistance = true,
    ShowTeam = true,
    ShowBoxes = true,
    ShowTracers = true,
    RainbowMode = false,
    RainbowSpeed = 2,
    MaxDistance = DISPLAY_DISTANCE,
    TextSize = 14,
    TextFont = Enum.Font.Code,
    BoxesEnabled = true,
    TracersEnabled = true,
    HighlightEnabled = true,
    ChamsEnabled = true,
    ShowWeapons = true,
    ShowAimlines = true,
    BoxThickness = 2,
    BoxType = "3D",
    ESPThickness = 2,
    ShowSkeletons = true,
    HideTeammates = false,
    HeadshotMode = false,
    PingCheck = true,
    ShowArmorStatus = true,
    NamePosition = "Top",
    TransparencySmoothing = true,
    RadarEnabled = true,
    RadarSize = RADAR_SIZE,
    RadarPosition = RADAR_POSITION,
    RadarTransparency = 0.3,
    RadarZoom = 1.0,
    RadarDotSize = 4,
    ShowInvisible = true,
    AlertMode = false,
    AlertDistance = ALERT_DISTANCE,
    CriticalHealthThreshold = CRITICAL_HEALTH_THRESHOLD,
    TeamCheckEnabled = true,
    VoiceIndicator = true,
    AIDetection = true,
    ColorByDistance = true,
    ColorByRole = false,
    PluginSystem = true,
    Wallhack = false,
    XRayMode = false,
    PredictionLines = false,
    ESPFade = false,
    AutoAdjustDistance = false,
    PerformanceMode = false,
    CustomCrosshair = false,
    CrosshairSize = 20,
    CrosshairColor = Color3.new(1, 0, 0),
    CrosshairThickness = 2,
    LootESP = false,
    VehicleESP = false,
    SilentAimIndicator = false,
    FOVCircle = false,
    FOVRadius = 100,
    Hitmarkers = false,
    KillEffect = false,
    SoundESP = false,
    AntiAFK = false,
    AutoSaveConfig = true,
    Theme = "Dark",
    UpdateFrequency = 0.1,
    AimbotEnabled = false,
    AimbotFOV = 30,
    AimbotSmoothing = 0.1,
    FilterList = {},
    HitmarkerHeadshotColor = Color3.fromRGB(255, 100, 0),
    HitmarkerBodyColor = Color3.fromRGB(255, 0, 0)
}

-- ESP Container with obfuscation
local ESPContainer = Instance.new("Folder")
ESPContainer.Name = HttpService:GenerateGUID(false)
ESPContainer.Parent = CoreGui

-- Utility Functions
local Utility = {
    GetDistance = function(position1, position2)
        return (position1 - position2).Magnitude
    end,
    
    FormatDistance = function(distance)
        return string.format("%.1f", distance)
    end,
    
    GetHealthData = function(humanoid)
        if not humanoid or not humanoid.Parent then return {Percentage = 0, Color = Color3.new(1,0,0), Text = "Dead"} end
        local health = humanoid.Health
        local maxHealth = humanoid.MaxHealth
        local percentage = (health / maxHealth) * 100
        local color = Color3.fromRGB(255 * (1 - health / maxHealth), 255 * (health / maxHealth), 0)
        return {Percentage = percentage, Color = color, Text = string.format("%.0f/%.0f HP", health, maxHealth)}
    end,
    
    IsTeamMate = function(player)
        if not Config.TeamCheckEnabled then return false end
        return player.Team and LOCAL_PLAYER.Team and player.Team == LOCAL_PLAYER.Team
    end,
    
    RainbowColor = function()
        local time = tick() * Config.RainbowSpeed
        return Color3.fromHSV(time % 1, 1, 1)
    end,
    
    GetCharacter = function(player)
        return player.Character or player.CharacterAdded:Wait()
    end,
    
    IsVisible = function(from, to)
        local raycastParams = RaycastParams.new()
        raycastParams.FilterDescendantsInstances = {LOCAL_PLAYER.Character}
        raycastParams.FilterType = Enum.RaycastFilterType.Blacklist
        local result = workspace:Raycast(from, (to - from).Unit * 1000, raycastParams)
        return not result or (result.Position - to).Magnitude < 5
    end,
    
    IsAI = function(character)
        if not character then return false end
        local humanoid = character:FindFirstChildOfClass("Humanoid")
        if not humanoid then return false end
        return character:FindFirstChild("AI") or character:FindFirstChild("Bot") or character:FindFirstChild("NPC") or not Players:GetPlayerFromCharacter(character)
    end,
    
    IsCriticalHealth = function(humanoid)
        if not humanoid then return false end
        return (humanoid.Health / humanoid.MaxHealth) * 100 <= Config.CriticalHealthThreshold
    end,
    
    GetColorByDistance = function(distance)
        local maxDist = Config.MaxDistance
        if distance <= maxDist * 0.3 then return Color3.fromRGB(0, 255, 0)
        elseif distance <= maxDist * 0.6 then return Color3.fromRGB(255, 255, 0)
        else return Color3.fromRGB(255, 0, 0) end
    end,
    
    IsSpeaking = function(player)
        local voiceService = game:GetService("VoiceChatService")
        if voiceService then
            local voiceState = voiceService:GetPlayerVoiceState(player)
            return voiceState and voiceState.isVoiceEnabled and voiceState.isSpeaking
        end
        return false
    end,
    
    PredictPosition = function(part)
        local velocity = part.Velocity
        return part.Position + velocity * 0.1
    end,
    
    GetTransparency = function(distance)
        if not Config.ESPFade then return 0 end
        return math.clamp(distance / Config.MaxDistance, 0, 0.9)
    end,
    
    AutoAdjustMaxDistance = function()
        local closest = math.huge
        for _, player in ipairs(Players:GetPlayers()) do
            if player ~= LOCAL_PLAYER and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                local dist = Utility.GetDistance(LOCAL_PLAYER.Character.HumanoidRootPart.Position, player.Character.HumanoidRootPart.Position)
                closest = math.min(closest, dist)
            end
        end
        return math.max(100, math.min(DISPLAY_DISTANCE, closest * 1.5))
    end,
    
    IsLoot = function(part)
        return part.Name:lower():match("loot") or part.Name:lower():match("item") or part:FindFirstChild("Pickup")
    end,
    
    IsVehicle = function(part)
        return part:FindFirstChild("VehicleSeat") or part.Name:lower():match("vehicle")
    end
}

-- ESP Class
local ESP = {
    Objects = {},
    Connections = {},
    CharacterConnections = {},
    LootObjects = {},
    VehicleObjects = {},
    SoundSources = {},
    Hitmarkers = {},
    LastUpdates = {}
}

function ESP:SetupCharacterConnections(player)
    if self.CharacterConnections[player] then self.CharacterConnections[player]:Disconnect() end
    
    self.CharacterConnections[player] = player.CharacterAdded:Connect(function(character)
        task.wait(0.1)
        if self.Objects[player] then
            self:UpdateESP(player, character)
        else
            self:CreatePlayerESP(player)
        end
    end)
end

function ESP:UpdateESP(player, character)
    local espData = self.Objects[player]
    if not espData then return end
    
    espData.Highlight.Adornee = character
    espData.Highlight.Enabled = Config.Enabled and Config.HighlightEnabled
    
    local rootPart = character and character:WaitForChild("HumanoidRootPart", 3)
    if rootPart then
        espData.BillboardGui.Adornee = rootPart
        espData.BillboardGui.Enabled = Config.Enabled
    else
        espData.BillboardGui.Enabled = false
        espData.Highlight.Enabled = false
    end
    
    if espData.Skeleton then
        espData.Skeleton.Folder:Destroy()
        espData.Skeleton = self:CreateSkeletonESP(character, espData.Container)
    end
    
    if espData.PredictionLine then
        espData.PredictionLine:Destroy()
        espData.PredictionLine = self:CreatePredictionLine(character, espData.Container)
    end
end

function ESP:CreateSkeletonESP(character, container)
    if not Config.ShowSkeletons or not character then return nil end
    
    local skeletonFolder = Instance.new("Folder")
    skeletonFolder.Name = "Skeleton_" .. HttpService:GenerateGUID(false)
    skeletonFolder.Parent = container
    
    local skeletonParts = {
        {"Head", "UpperTorso"}, {"UpperTorso", "LowerTorso"}, {"UpperTorso", "LeftUpperArm"},
        {"LeftUpperArm", "LeftLowerArm"}, {"LeftLowerArm", "LeftHand"}, {"UpperTorso", "RightUpperArm"},
        {"RightUpperArm", "RightLowerArm"}, {"RightLowerArm", "RightHand"}, {"LowerTorso", "LeftUpperLeg"},
        {"LeftUpperLeg", "LeftLowerLeg"}, {"LeftLowerLeg", "LeftFoot"}, {"LowerTorso", "RightUpperLeg"},
        {"RightUpperLeg", "RightLowerLeg"}, {"RightLowerLeg", "RightFoot"}
    }
    
    local lines = {}
    for _, connection in ipairs(skeletonParts) do
        local line = Instance.new("LineHandleAdornment")
        line.Name = connection[1] .. "_to_" .. connection[2]
        line.Thickness = 1.5
        line.Color3 = Color3.new(1, 1, 1)
        line.AlwaysOnTop = Config.Wallhack
        line.ZIndex = 5
        line.Visible = false
        line.Parent = skeletonFolder
        line.Adornee = character
        table.insert(lines, {Line = line, From = connection[1], To = connection[2]})
    end
    
    return {Folder = skeletonFolder, Lines = lines}
end

function ESP:CreatePredictionLine(character, container)
    if not Config.PredictionLines or not character then return nil end
    local line = Instance.new("LineHandleAdornment")
    line.Name = "Prediction_" .. HttpService:GenerateGUID(false)
    line.Thickness = 2
    line.Color3 = Color3.fromRGB(0, 255, 255)
    line.AlwaysOnTop = Config.Wallhack
    line.ZIndex = 4
    line.Parent = container
    line.Adornee = character
    return line
end

function ESP:CreatePlayerESP(player)
    if player == LOCAL_PLAYER then return end
    if Config.FilterList[player.Name] then return end
    
    if self.Objects[player] then
        self:RemovePlayerESP(player)
    end
    
    local playerESPFolder = Instance.new("Folder")
    playerESPFolder.Name = player.Name .. "_" .. HttpService:GenerateGUID(false)
    playerESPFolder.Parent = ESPContainer
    
    local highlight = Instance.new("Highlight")
    highlight.Name = player.Name .. "_ESP_" .. HttpService:GenerateGUID(false)
    highlight.FillColor = TEAM_COLORS.Enemy
    highlight.OutlineColor = Color3.new(1, 1, 1)
    highlight.FillTransparency = Config.XRayMode and 0.7 or 0.5
    highlight.OutlineTransparency = 0
    highlight.Enabled = Config.Enabled and Config.HighlightEnabled
    
    local billboardGui = Instance.new("BillboardGui")
    billboardGui.Name = player.Name .. "_Info_" .. HttpService:GenerateGUID(false)
    billboardGui.AlwaysOnTop = Config.Wallhack
    billboardGui.Size = UDim2.new(0, 200, 0, 150)
    billboardGui.StudsOffset = Vector3.new(0, 3, 0)
    billboardGui.Enabled = Config.Enabled
    billboardGui.Parent = playerESPFolder
    
    local labels = {}
    local yOffset = 0
    
    local function createLabel(name, color)
        local label = Instance.new("TextLabel")
        label.Name = name
        label.Size = UDim2.new(1, 0, 0, 20)
        label.Position = UDim2.new(0, 0, 0, yOffset)
        label.BackgroundTransparency = 1
        label.TextColor3 = color or Color3.new(1, 1, 1)
        label.TextSize = Config.TextSize
        label.Font = Config.TextFont
        label.Text = ""
        label.Parent = billboardGui
        yOffset = yOffset + 20
        return label
    end
    
    labels.Name = createLabel("NameLabel")
    labels.Health = createLabel("HealthLabel")
    labels.Distance = createLabel("DistanceLabel")
    labels.Weapon = createLabel("WeaponLabel")
    labels.Armor = createLabel("ArmorLabel", TEAM_COLORS.Armor)
    
    local character = player.Character
    if not character then
        task.wait(1)
        character = player.Character or player.CharacterAdded:Wait()
    end
    highlight.Adornee = character
    highlight.Parent = playerESPFolder
    
    local rootPart = character and character:WaitForChild("HumanoidRootPart", 3)
    if rootPart then billboardGui.Adornee = rootPart end
    
    local skeletonESP = self:CreateSkeletonESP(character, playerESPFolder)
    local predictionLine = self:CreatePredictionLine(character, playerESPFolder)
    
    self.Objects[player] = {
        Highlight = highlight,
        BillboardGui = billboardGui,
        Container = playerESPFolder,
        Skeleton = skeletonESP,
        PredictionLine = predictionLine,
        Labels = labels
    }
    
    self.LastUpdates[player] = 0
    self:SetupCharacterConnections(player)
    
    if character and Config.Hitmarkers then
        local humanoid = character:FindFirstChild("Humanoid")
        if humanoid then
            humanoid.HealthChanged:Connect(function(health)
                if health < humanoid.MaxHealth then
                    local head = character:FindFirstChild("Head")
                    local damagePos = head and head.Position or rootPart and rootPart.Position
                    if damagePos then
                        local screenPos = workspace.CurrentCamera:WorldToViewportPoint(damagePos)
                        local isHeadshot = (damagePos == head and head.Position)
                        self:CreateHitmarker(UDim2.new(0, screenPos.X, 0, screenPos.Y), isHeadshot)
                    end
                end
            end)
        end
    end
    
    local connection = RunService.RenderStepped:Connect(function()
        if not Config.Enabled or not self.Objects[player] then return end
        if Config.FilterList[player.Name] then return end
        
        local character = player.Character
        local humanoid = character and character:FindFirstChild("Humanoid")
        local rootPart = character and character:FindFirstChild("HumanoidRootPart")
        
        local localCharacter = LOCAL_PLAYER.Character
        local localRootPart = localCharacter and localCharacter:FindFirstChild("HumanoidRootPart")
        local distance = localRootPart and Utility.GetDistance(localRootPart.Position, rootPart and rootPart.Position or localRootPart.Position) or 0
        
        local lastUpdate = self.LastUpdates[player] or 0
        if distance > 500 and tick() - lastUpdate < Config.UpdateFrequency then return end
        self.LastUpdates[player] = tick()
        
        if not character or not rootPart then
            billboardGui.Enabled = false
            highlight.Enabled = false
            labels.Name.Text = player.Name
            labels.Health.Text = "Dead"
            labels.Distance.Text = Utility.FormatDistance(distance) .. " studs"
            labels.Weapon.Text = "N/A"
            labels.Armor.Text = "N/A"
            return
        end
        
        if Config.HideTeammates and Utility.IsTeamMate(player) then
            billboardGui.Enabled = false
            highlight.Enabled = false
            return
        end
        
        -- Ensure Highlight is always properly set
        if not highlight.Parent then
            highlight.Parent = playerESPFolder
        end
        highlight.Adornee = character
        highlight.Enabled = Config.Enabled and Config.HighlightEnabled
        
        billboardGui.Adornee = rootPart
        billboardGui.Enabled = Config.Enabled
        
        if Config.AutoAdjustDistance then Config.MaxDistance = Utility.AutoAdjustMaxDistance() end
        if distance > Config.MaxDistance then
            billboardGui.Enabled = false
            highlight.Enabled = false
            return
        end
        
        highlight.FillTransparency = Utility.GetTransparency(distance)
        
        local espColor = TEAM_COLORS.Enemy
        local isVisible = Config.ShowInvisible and localRootPart and Utility.IsVisible(localRootPart.Position, rootPart.Position)
        local isCritical = humanoid and Utility.IsCriticalHealth(humanoid)
        local isAI = Utility.IsAI(character)
        
        if Config.RainbowMode then espColor = Utility.RainbowColor()
        elseif Config.ColorByRole and player:GetRoleInGroup(game.PlaceId) == "VIP" then espColor = TEAM_COLORS.VIP
        elseif Config.ColorByRole and player:GetRankInGroup(game.PlaceId) >= 200 then espColor = TEAM_COLORS.Admin
        elseif Config.HeadshotMode and distance < 20 then espColor = TEAM_COLORS.Headshot
        elseif isAI and Config.AIDetection then espColor = TEAM_COLORS.AI
        elseif isCritical then espColor = TEAM_COLORS.Critical
        elseif Config.AlertMode and distance <= Config.AlertDistance then espColor = TEAM_COLORS.Alert
        elseif Config.ColorByDistance then espColor = Utility.GetColorByDistance(distance)
        else espColor = Utility.IsTeamMate(player) and TEAM_COLORS.Friendly or TEAM_COLORS.Enemy end
        
        highlight.FillColor = espColor
        
        local healthData = Utility.GetHealthData(humanoid)
        
        labels.Name.Visible = Config.ShowName
        if Config.ShowName then labels.Name.Text = player.Name end
        
        labels.Health.Visible = Config.ShowHealth
        if Config.ShowHealth then 
            labels.Health.Text = healthData.Text 
            labels.Health.TextColor3 = healthData.Color 
        end
        
        labels.Distance.Visible = Config.ShowDistance
        if Config.ShowDistance then labels.Distance.Text = Utility.FormatDistance(distance) .. " studs" end
        
        labels.Weapon.Visible = Config.ShowWeapons
        if Config.ShowWeapons then
            local weapon = character:FindFirstChildWhichIsA("Tool")
            labels.Weapon.Text = weapon and "Weapon: " .. weapon.Name or "No Weapon"
        end
        
        labels.Armor.Visible = Config.ShowArmorStatus
        if Config.ShowArmorStatus then
            local hasArmor = character:FindFirstChild("Armor") or character:FindFirstChild("Shield") or (humanoid and humanoid:FindFirstChild("Armor"))
            labels.Armor.Text = hasArmor and "Armored" or "No Armor"
        end
        
        if Config.ShowSkeletons and self.Objects[player].Skeleton then
            for _, bone in ipairs(self.Objects[player].Skeleton.Lines) do
                local fromPart = character:FindFirstChild(bone.From)
                local toPart = character:FindFirstChild(bone.To)
                if fromPart and toPart then
                    local line = bone.Line
                    line.Visible = true
                    line.Color3 = espColor
                    line.Length = (fromPart.Position - toPart.Position).Magnitude
                    line.CFrame = CFrame.new(fromPart.Position, toPart.Position) * CFrame.new(0, 0, -line.Length/2)
                else bone.Line.Visible = false end
            end
        end
        
        if Config.PredictionLines and self.Objects[player].PredictionLine then
            local predictedPos = Utility.PredictPosition(rootPart)
            local line = self.Objects[player].PredictionLine
            line.Length = (rootPart.Position - predictedPos).Magnitude
            line.CFrame = CFrame.new(rootPart.Position, predictedPos) * CFrame.new(0, 0, -line.Length/2)
            line.Visible = true
        end
    end)
    
    self.Connections[player] = connection
end

function ESP:CreateLootESP(part)
    local highlight = Instance.new("Highlight")
    highlight.Name = part.Name .. "_Loot_" .. HttpService:GenerateGUID(false)
    highlight.FillColor = TEAM_COLORS.Loot
    highlight.OutlineColor = Color3.new(1, 1, 1)
    highlight.FillTransparency = 0.5
    highlight.OutlineTransparency = 0
    highlight.Enabled = Config.LootESP
    highlight.Adornee = part
    highlight.Parent = ESPContainer
    
    self.LootObjects[part] = {Highlight = highlight}
end

function ESP:CreateVehicleESP(part)
    local highlight = Instance.new("Highlight")
    highlight.Name = part.Name .. "_Vehicle_" .. HttpService:GenerateGUID(false)
    highlight.FillColor = TEAM_COLORS.Vehicle
    highlight.OutlineColor = Color3.new(1, 1, 1)
    highlight.FillTransparency = 0.5
    highlight.OutlineTransparency = 0
    highlight.Enabled = Config.VehicleESP
    highlight.Adornee = part
    highlight.Parent = ESPContainer
    
    self.VehicleObjects[part] = {Highlight = highlight}
end

function ESP:CreateRadar()
    local screenGui = Instance.new("ScreenGui")
    screenGui.Name = "Radar_" .. HttpService:GenerateGUID(false)
    screenGui.ResetOnSpawn = false
    screenGui.Parent = CoreGui
    
    local radar = Instance.new("Frame")
    radar.Name = "RadarFrame"
    radar.Size = UDim2.new(0, Config.RadarSize, 0, Config.RadarSize)
    radar.Position = Config.RadarPosition
    radar.BackgroundColor3 = Config.Theme == "Dark" and Color3.fromRGB(0, 0, 0) or Color3.fromRGB(200, 200, 200)
    radar.BackgroundTransparency = Config.RadarTransparency
    radar.BorderSizePixel = 2
    radar.BorderColor3 = Color3.fromRGB(100, 100, 100)
    radar.ZIndex = 10
    radar.Visible = Config.RadarEnabled
    radar.Parent = screenGui
    
    local centerDot = Instance.new("Frame")
    centerDot.Name = "Center"
    centerDot.Size = UDim2.new(0, Config.RadarDotSize, 0, Config.RadarDotSize)
    centerDot.Position = UDim2.new(0.5, -Config.RadarDotSize/2, 0.5, -Config.RadarDotSize/2)
    centerDot.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    centerDot.BorderSizePixel = 0
    centerDot.ZIndex = 11
    centerDot.Parent = radar
    
    self.Radar = {ScreenGui = screenGui, Frame = radar, Dots = {}, UpdateConnection = nil}
    self:UpdateRadarDots()
    
    local connection = RunService.RenderStepped:Connect(function()
        if not Config.RadarEnabled or not self.Radar then radar.Visible = false return end
        radar.Visible = true
        radar.Size = UDim2.new(0, Config.RadarSize, 0, Config.RadarSize)
        radar.Position = Config.RadarPosition
        radar.BackgroundTransparency = Config.RadarTransparency
        self:UpdateRadarPositions()
    end)
    
    self.Radar.UpdateConnection = connection
    return self.Radar
end

function ESP:UpdateRadarDots()
    if not self.Radar then return end
    
    for _, dot in pairs(self.Radar.Dots) do dot.Frame:Destroy() end
    self.Radar.Dots = {}
    
    for _, player in ipairs(Players:GetPlayers()) do
        if player ~= LOCAL_PLAYER then
            local playerDot = Instance.new("Frame")
            playerDot.Name = player.Name .. "_Dot_" .. HttpService:GenerateGUID(false)
            playerDot.Size = UDim2.new(0, Config.RadarDotSize, 0, Config.RadarDotSize)
            playerDot.BackgroundColor3 = Utility.IsTeamMate(player) and TEAM_COLORS.Friendly or TEAM_COLORS.Enemy
            playerDot.BorderSizePixel = 0
            playerDot.ZIndex = 11
            playerDot.Parent = self.Radar.Frame
            self.Radar.Dots[player] = {Frame = playerDot, Player = player}
        end
    end
end

function ESP:UpdateRadarPositions()
    if not self.Radar or not Config.RadarEnabled then return end
    
    local localCharacter = LOCAL_PLAYER.Character
    local localRootPart = localCharacter and localCharacter:FindFirstChild("HumanoidRootPart")
    if not localRootPart then return end
    
    local radarCenter = Vector2.new(self.Radar.Frame.AbsoluteSize.X/2, self.Radar.Frame.AbsoluteSize.Y/2)
    local radarSize = self.Radar.Frame.AbsoluteSize.X
    local radarRange = 100 * Config.RadarZoom
    
    local localCF = localRootPart.CFrame
    local facing = Vector2.new(localCF.LookVector.X, localCF.LookVector.Z)
    
    for player, dot in pairs(self.Radar.Dots) do
        local character = player.Character
        local rootPart = character and character:FindFirstChild("HumanoidRootPart")
        
        if rootPart and dot.Frame then
            local relativePos = rootPart.Position - localRootPart.Position
            local relativePosOnRadar = Vector2.new(relativePos.X, relativePos.Z)
            local angle = math.atan2(facing.Y, facing.X)
            local rotatedPos = Vector2.new(
                relativePosOnRadar.X * math.cos(-angle) - relativePosOnRadar.Y * math.sin(-angle),
                relativePosOnRadar.X * math.sin(-angle) + relativePosOnRadar.Y * math.cos(-angle)
            )
            
            local scaledPos = rotatedPos * (radarSize / (radarRange * 2))
            local finalPosX = math.clamp(radarCenter.X + scaledPos.X, Config.RadarDotSize/2, radarSize - Config.RadarDotSize/2)
            local finalPosY = math.clamp(radarCenter.Y + scaledPos.Y, Config.RadarDotSize/2, radarSize - Config.RadarDotSize/2)
            
            dot.Frame.Position = UDim2.new(0, finalPosX, 0, finalPosY)
            
            local humanoid = character:FindFirstChildOfClass("Humanoid")
            local distance = (rootPart.Position - localRootPart.Position).Magnitude
            
            local dotColor = Utility.IsTeamMate(player) and TEAM_COLORS.Friendly or TEAM_COLORS.Enemy
            if Config.ColorByRole and player:GetRoleInGroup(game.PlaceId) == "VIP" then dotColor = TEAM_COLORS.VIP
            elseif Config.ColorByRole and player:GetRankInGroup(game.PlaceId) >= 200 then dotColor = TEAM_COLORS.Admin
            elseif Utility.IsAI(character) then dotColor = TEAM_COLORS.AI
            elseif Config.ColorByDistance then dotColor = Utility.GetColorByDistance(distance)
            elseif humanoid and Utility.IsCriticalHealth(humanoid) then dotColor = TEAM_COLORS.Critical
            elseif Config.AlertMode and distance <= Config.AlertDistance then dotColor = TEAM_COLORS.Alert
            elseif Config.VoiceIndicator and Utility.IsSpeaking(player) then dotColor = Color3.fromRGB(255, 255, 255) end
            
            dot.Frame.BackgroundColor3 = dotColor
            dot.Frame.Size = Config.AlertMode and distance <= Config.AlertDistance and 
                UDim2.new(0, Config.RadarDotSize + 4, 0, Config.RadarDotSize + 4) or 
                UDim2.new(0, Config.RadarDotSize, 0, Config.RadarDotSize)
        else 
            dot.Frame.Visible = false 
        end
    end
end

function ESP:CreateCrosshair()
    local screenGui = Instance.new("ScreenGui")
    screenGui.Name = "Crosshair_" .. HttpService:GenerateGUID(false)
    screenGui.Parent = CoreGui
    
    local horizontal = Instance.new("Frame")
    horizontal.Size = UDim2.new(0, Config.CrosshairSize, 0, Config.CrosshairThickness)
    horizontal.Position = UDim2.new(0.5, -Config.CrosshairSize/2, 0.5, -Config.CrosshairThickness/2)
    horizontal.BackgroundColor3 = Config.CrosshairColor
    horizontal.BorderSizePixel = 0
    horizontal.Parent = screenGui
    
    local vertical = Instance.new("Frame")
    vertical.Size = UDim2.new(0, Config.CrosshairThickness, 0, Config.CrosshairSize)
    vertical.Position = UDim2.new(0.5, -Config.CrosshairThickness/2, 0.5, -Config.CrosshairSize/2)
    vertical.BackgroundColor3 = Config.CrosshairColor
    vertical.BorderSizePixel = 0
    vertical.Parent = screenGui
    
    self.Crosshair = {Gui = screenGui, Horizontal = horizontal, Vertical = vertical}
end

function ESP:CreateFOVCircle()
    local screenGui = Instance.new("ScreenGui")
    screenGui.Name = "FOV_" .. HttpService:GenerateGUID(false)
    screenGui.Parent = CoreGui
    
    local circle = Instance.new("Frame")
    circle.Size = UDim2.new(0, Config.FOVRadius * 2, 0, Config.FOVRadius * 2)
    circle.Position = UDim2.new(0.5, -Config.FOVRadius, 0.5, -Config.FOVRadius)
    circle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    circle.BackgroundTransparency = 0.9
    circle.BorderSizePixel = 0
    circle.ZIndex = 5
    circle.Parent = screenGui
    
    local uiCorner = Instance.new("UICorner")
    uiCorner.CornerRadius = UDim.new(1, 0)
    uiCorner.Parent = circle
    
    self.FOVCircle = {Gui = screenGui, Circle = circle}
end

function ESP:CreateHitmarker(position, isHeadshot)
    local screenGui = Instance.new("ScreenGui")
    screenGui.Name = "Hitmarker_" .. HttpService:GenerateGUID(false)
    screenGui.Parent = CoreGui
    
    local marker = Instance.new("Frame")
    marker.Size = UDim2.new(0, 10, 0, 10)
    marker.Position = position or UDim2.new(0.5, 0, 0.5, 0)
    marker.BackgroundColor3 = isHeadshot and Config.HitmarkerHeadshotColor or Config.HitmarkerBodyColor
    marker.BorderSizePixel = 0
    marker.Parent = screenGui
    
    table.insert(self.Hitmarkers, {Gui = screenGui, Marker = marker})
    task.delay(0.5, function()
        screenGui:Destroy()
        for i, hit in ipairs(self.Hitmarkers) do
            if hit.Gui == screenGui then
                table.remove(self.Hitmarkers, i)
                break
            end
        end
    end)
end

function ESP:UpdateAimbot()
    if not Config.AimbotEnabled then return end
    
    local closest, closestDist = nil, Config.AimbotFOV
    for player, _ in pairs(self.Objects) do
        if Config.FilterList[player.Name] or Utility.IsTeamMate(player) then continue end
        local root = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
        if root then
            local screenPos, onScreen = workspace.CurrentCamera:WorldToViewportPoint(root.Position)
            local dist = (Vector2.new(screenPos.X, screenPos.Y) - Vector2.new(workspace.CurrentCamera.ViewportSize.X/2, workspace.CurrentCamera.ViewportSize.Y/2)).Magnitude
            if onScreen and dist < closestDist then
                closest, closestDist = root, dist
            end
        end
    end
    if closest then
        workspace.CurrentCamera.CFrame = workspace.CurrentCamera.CFrame:Lerp(
            CFrame.new(workspace.CurrentCamera.CFrame.Position, closest.Position), 
            Config.AimbotSmoothing
        )
    end
end

function ESP:UpdateSoundESP()
    if not Config.SoundESP then return end
    
    for _, sound in ipairs(workspace:GetDescendants()) do
        if sound:IsA("Sound") and sound.IsPlaying and not self.SoundSources[sound] then
            local pos = sound.Parent and sound.Parent.Position
            if pos then
                local billboard = Instance.new("BillboardGui")
                billboard.Name = "Sound_" .. HttpService:GenerateGUID(false)
                billboard.Size = UDim2.new(0, 50, 0, 50)
                billboard.StudsOffset = Vector3.new(0, 2, 0)
                billboard.AlwaysOnTop = true
                billboard.Adornee = sound.Parent
                billboard.Parent = ESPContainer
                
                local frame = Instance.new("Frame")
                frame.Size = UDim2.new(1, 0, 1, 0)
                frame.BackgroundColor3 = Color3.fromRGB(0, 255, 255)
                frame.BackgroundTransparency = 0.7
                frame.Parent = billboard
                
                local uiCorner = Instance.new("UICorner")
                uiCorner.CornerRadius = UDim.new(0.5, 0)
                uiCorner.Parent = frame
                
                self.SoundSources[sound] = {Gui = billboard}
                
                sound.Stopped:Connect(function()
                    if self.SoundSources[sound] then
                        self.SoundSources[sound].Gui:Destroy()
                        self.SoundSources[sound] = nil
                    end
                end)
            end
        end
    end
end

function ESP:MonitorPlayers()
    task.spawn(function()
        while true do
            for _, player in ipairs(Players:GetPlayers()) do
                if player ~= LOCAL_PLAYER and not self.Objects[player] then
                    self:CreatePlayerESP(player)
                    if self.Radar then self:UpdateRadarDots() end
                elseif player ~= LOCAL_PLAYER and self.Objects[player] and self.Objects[player].Highlight.Parent == nil then
                    -- Recreate highlight if it's missing
                    self:RemovePlayerESP(player)
                    self:CreatePlayerESP(player)
                end
            end
            task.wait(1)
        end
    end)
end

function ESP:Initialize()
    task.wait(1)
    
    local function createESPForAllPlayers()
        for _, player in ipairs(Players:GetPlayers()) do
            if player ~= LOCAL_PLAYER and not self.Objects[player] then
                self:CreatePlayerESP(player)
            end
        end
    end
    
    createESPForAllPlayers()
    
    self:MonitorPlayers()
    
    local inputConnection
    inputConnection = UserInputService.InputBegan:Connect(function(input)
        if input.KeyCode == Enum.KeyCode.RightControl then
            Config.Enabled = not Config.Enabled
            for _, espData in pairs(self.Objects) do
                espData.Highlight.Enabled = Config.Enabled and Config.HighlightEnabled
                espData.BillboardGui.Enabled = Config.Enabled
            end
            game.StarterGui:SetCore("SendNotification", {
                Title = "ESP " .. (Config.Enabled and "Enabled" or "Disabled"), 
                Text = "Right Control to toggle", 
                Duration = 2
            })
        elseif input.KeyCode == Enum.KeyCode.RightAlt then
            Config.RainbowMode = not Config.RainbowMode
            game.StarterGui:SetCore("SendNotification", {
                Title = "Rainbow Mode " .. (Config.RainbowMode and "Enabled" or "Disabled"), 
                Text = "Right Alt to toggle", 
                Duration = 2
            })
        elseif input.KeyCode == Enum.KeyCode.End then
            Config.HeadshotMode = not Config.HeadshotMode
            game.StarterGui:SetCore("SendNotification", {
                Title = "Headshot Mode " .. (Config.HeadshotMode and "Enabled" or "Disabled"), 
                Text = "End to toggle", 
                Duration = 2
            })
        elseif input.KeyCode == Enum.KeyCode.Home then
            Config.ChamsEnabled = not Config.ChamsEnabled
            for _, espData in pairs(self.Objects) do
                espData.Highlight.Enabled = Config.Enabled and Config.ChamsEnabled
            end
            game.StarterGui:SetCore("SendNotification", {
                Title = "Chams " .. (Config.ChamsEnabled and "Enabled" or "Disabled"), 
                Text = "Home to toggle", 
                Duration = 2
            })
        elseif input.KeyCode == Enum.KeyCode.F4 then
            if self.ConfigGui then
                local frame = self.ConfigGui.MainFrame
                frame.Visible = not frame.Visible
                if frame.Visible then
                    TweenService:Create(frame, TweenInfo.new(0.3), {Position = UDim2.new(0.5, -150, 0.5, -200)}):Play()
                else
                    TweenService:Create(frame, TweenInfo.new(0.3), {Position = UDim2.new(0.5, -150, 1.5, 0)}):Play()
                end
            end
        elseif input.KeyCode == Enum.KeyCode.Insert then
            Config.AimbotEnabled = not Config.AimbotEnabled
            game.StarterGui:SetCore("SendNotification", {
                Title = "Aimbot " .. (Config.AimbotEnabled and "Enabled" or "Disabled"), 
                Text = "Insert to toggle", 
                Duration = 2
            })
        end
    end)
    
    self.InputConnection = inputConnection
    
    if Config.RadarEnabled then self:CreateRadar() end
    if Config.CustomCrosshair then self:CreateCrosshair() end
    if Config.FOVCircle then self:CreateFOVCircle() end
    
    Players.PlayerAdded:Connect(function(player)
        if player ~= LOCAL_PLAYER then
            task.spawn(function()
                task.wait(1)
                if not self.Objects[player] then
                    self:CreatePlayerESP(player)
                    if self.Radar then self:UpdateRadarDots() end
                end
            end)
        end
    end)
    
    Players.PlayerRemoving:Connect(function(player)
        self:RemovePlayerESP(player)
    end)
    
    workspace.DescendantAdded:Connect(function(descendant)
        if Config.LootESP and Utility.IsLoot(descendant) then 
            self:CreateLootESP(descendant)
        elseif Config.VehicleESP and Utility.IsVehicle(descendant) then 
            self:CreateVehicleESP(descendant) 
        end
    end)
    
    if Config.AntiAFK then
        task.spawn(function()
            while Config.AntiAFK and task.wait(60) do
                pcall(function()
                    UserInputService:SendMouseMovement(Vector2.new(1, 1))
                end)
            end
        end)
    end
    
    if Config.AutoSaveConfig then
        task.spawn(function()
            while task.wait(300) and Config.AutoSaveConfig do
                pcall(function()
                    game:HttpPost("http://yourserver/config.json", HttpService:JSONEncode(Config))
                end)
            end
        end)
    end
    
    RunService.RenderStepped:Connect(function()
        self:UpdateAimbot()
        self:UpdateSoundESP()
    end)
    
    self:CreateConfigGUI()
    if Config.AlertMode then self:CreateAlertSystem() end
end

function ESP:RemovePlayerESP(player)
    if self.Objects[player] then
        self.Objects[player].Container:Destroy()
        if self.Connections[player] then self.Connections[player]:Disconnect() end
        if self.CharacterConnections[player] then self.CharacterConnections[player]:Disconnect() end
        self.Connections[player] = nil
        self.CharacterConnections[player] = nil
        self.Objects[player] = nil
        self.LastUpdates[player] = nil
        if self.Radar then self:UpdateRadarDots() end
    end
end

function ESP:CreateAlertSystem()
    local alertSound = Instance.new("Sound")
    alertSound.Name = "Alert_" .. HttpService:GenerateGUID(false)
    alertSound.SoundId = "rbxassetid://6518811702"
    alertSound.Volume = 0.5
    alertSound.Parent = CoreGui
    
    local alertGui = Instance.new("ScreenGui")
    alertGui.Name = "AlertGui_" .. HttpService:GenerateGUID(false)
    alertGui.Parent = CoreGui
    
    local alertFrame = Instance.new("Frame")
    alertFrame.Name = "AlertFrame"
    alertFrame.Size = UDim2.new(0, 200, 0, 50)
    alertFrame.Position = UDim2.new(0.5, -100, 0.2, 0)
    alertFrame.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
    alertFrame.BackgroundTransparency = 0.3
    alertFrame.BorderSizePixel = 2
    alertFrame.Visible = false
    alertFrame.Parent = alertGui
    
    local alertText = Instance.new("TextLabel")
    alertText.Name = "AlertText"
    alertText.Size = UDim2.new(1, 0, 1, 0)
    alertText.BackgroundTransparency = 1
    alertText.TextColor3 = Color3.new(1, 1, 1)
    alertText.Font = Enum.Font.SourceSansBold
    alertText.TextSize = 18
    alertText.Text = "ENEMY NEARBY!"
    alertText.Parent = alertFrame
    
    self.AlertConnection = RunService.Heartbeat:Connect(function()
        if not Config.Enabled or not Config.AlertMode then 
            alertFrame.Visible = false 
            return 
        end
        
        local localCharacter = LOCAL_PLAYER.Character
        local localRootPart = localCharacter and localCharacter:FindFirstChild("HumanoidRootPart")
        if not localRootPart then 
            alertFrame.Visible = false 
            return 
        end
        
        local enemyDetected = false
        local closestDistance = Config.AlertDistance
        local closestEnemy = nil
        
        for player, _ in pairs(self.Objects) do
            if not Utility.IsTeamMate(player) then
                local character = player.Character
                local rootPart = character and character:FindFirstChild("HumanoidRootPart")
                if rootPart then
                    local distance = Utility.GetDistance(localRootPart.Position, rootPart.Position)
                    if distance <= Config.AlertDistance and distance < closestDistance then
                        enemyDetected = true
                        closestDistance = distance
                        closestEnemy = player
                    end
                end
            end
        end
        
        if enemyDetected and closestEnemy then
            if not alertFrame.Visible then alertSound:Play() end
            alertFrame.Visible = true
            alertText.Text = string.format("ENEMY NEARBY: %s (%s studs)", closestEnemy.Name, Utility.FormatDistance(closestDistance))
            alertFrame.BackgroundTransparency = (math.sin(tick() * 5) + 1) / 4 + 0.5
        else 
            alertFrame.Visible = false 
        end
    end)
    
    self.AlertSystem = {Sound = alertSound, Gui = alertGui, Connection = self.AlertConnection}
end

function ESP:CreateConfigGUI()
    local configGui = Instance.new("ScreenGui")
    configGui.Name = "Config_" .. HttpService:GenerateGUID(false)
    configGui.Parent = CoreGui
    
    local mainFrame = Instance.new("Frame")
    mainFrame.Name = "MainFrame"
    mainFrame.Size = UDim2.new(0, 300, 0, 400)
    mainFrame.Position = UDim2.new(0.5, -150, 1.5, 0)
    mainFrame.BackgroundColor3 = Config.Theme == "Dark" and Color3.fromRGB(30, 30, 30) or Color3.fromRGB(200, 200, 200)
    mainFrame.BorderColor3 = Config.Theme == "Dark" and Color3.fromRGB(50, 50, 50) or Color3.fromRGB(100, 100, 100)
    mainFrame.BorderSizePixel = 2
    mainFrame.Parent = configGui
    
    local titleLabel = Instance.new("TextLabel")
    titleLabel.Name = "TitleLabel"
    titleLabel.Size = UDim2.new(1, 0, 0, 30)
    titleLabel.BackgroundColor3 = Config.Theme == "Dark" and Color3.fromRGB(40, 40, 40) or Color3.fromRGB(180, 180, 180)
    titleLabel.BorderSizePixel = 0
    titleLabel.TextColor3 = Config.Theme == "Dark" and Color3.new(1, 1, 1) or Color3.new(0, 0, 0)
    titleLabel.Font = Enum.Font.SourceSansBold
    titleLabel.TextSize = 18
    titleLabel.Text = "ESP Configuration"
    titleLabel.Parent = mainFrame
    
    local scrollFrame = Instance.new("ScrollingFrame")
    scrollFrame.Name = "OptionsScroll"
    scrollFrame.Size = UDim2.new(1, -10, 1, -40)
    scrollFrame.Position = UDim2.new(0, 5, 0, 35)
    scrollFrame.BackgroundTransparency = 1
    scrollFrame.BorderSizePixel = 0
    scrollFrame.ScrollBarThickness = 6
    scrollFrame.CanvasSize = UDim2.new(0, 0, 0, 1400)
    scrollFrame.Parent = mainFrame
    
    local yPos = 0
    local function addToggle(name, configKey)
        local toggleFrame = Instance.new("Frame")
        toggleFrame.Size = UDim2.new(1, -10, 0, 30)
        toggleFrame.Position = UDim2.new(0, 0, 0, yPos)
        toggleFrame.BackgroundTransparency = 1
        toggleFrame.Parent = scrollFrame
        
        local nameLabel = Instance.new("TextLabel")
        nameLabel.Size = UDim2.new(0.7, 0, 1, 0)
        nameLabel.Position = UDim2.new(0, 5, 0, 0)
        nameLabel.BackgroundTransparency = 1
        nameLabel.TextColor3 = Config.Theme == "Dark" and Color3.new(1, 1, 1) or Color3.new(0, 0, 0)
        nameLabel.Font = Enum.Font.SourceSans
        nameLabel.TextSize = 16
        nameLabel.Text = name
        nameLabel.TextXAlignment = Enum.TextXAlignment.Left
        nameLabel.Parent = toggleFrame
        
        local toggleButton = Instance.new("TextButton")
        toggleButton.Size = UDim2.new(0.25, 0, 0.8, 0)
        toggleButton.Position = UDim2.new(0.75, 0, 0.1, 0)
        toggleButton.BackgroundColor3 = Config[configKey] and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 0, 0)
        toggleButton.BorderColor3 = Color3.new(1, 1, 1)
        toggleButton.Text = Config[configKey] and "ON" or "OFF"
        toggleButton.Font = Enum.Font.SourceSansBold
        toggleButton.TextSize = 14
        toggleButton.TextColor3 = Color3.new(1, 1, 1)
        toggleButton.Parent = toggleFrame
        
        toggleButton.MouseButton1Click:Connect(function()
            Config[configKey] = not Config[configKey]
            toggleButton.BackgroundColor3 = Config[configKey] and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 0, 0)
            toggleButton.Text = Config[configKey] and "ON" or "OFF"
            if configKey == "RadarEnabled" and Config.RadarEnabled and not self.Radar then 
                self:CreateRadar()
            elseif configKey == "CustomCrosshair" and Config.CustomCrosshair and not self.Crosshair then 
                self:CreateCrosshair()
            elseif configKey == "FOVCircle" and Config.FOVCircle and not self.FOVCircle then 
                self:CreateFOVCircle() 
            end
        end)
        
        yPos = yPos + 35
    end
    
    local function addSlider(name, configKey, min, max, increment)
        local sliderFrame = Instance.new("Frame")
        sliderFrame.Size = UDim2.new(1, -10, 0, 50)
        sliderFrame.Position = UDim2.new(0, 0, 0, yPos)
        sliderFrame.BackgroundTransparency = 1
        sliderFrame.Parent = scrollFrame
        
        local nameLabel = Instance.new("TextLabel")
        nameLabel.Size = UDim2.new(1, -80, 0, 20)
        nameLabel.Position = UDim2.new(0, 5, 0, 0)
        nameLabel.BackgroundTransparency = 1
        nameLabel.TextColor3 = Config.Theme == "Dark" and Color3.new(1, 1, 1) or Color3.new(0, 0, 0)
        nameLabel.Font = Enum.Font.SourceSans
        nameLabel.TextSize = 16
        nameLabel.Text = name .. ": " .. Config[configKey]
        nameLabel.TextXAlignment = Enum.TextXAlignment.Left
        nameLabel.Parent = sliderFrame
        
        local valueDisplay = Instance.new("TextLabel")
        valueDisplay.Size = UDim2.new(0, 60, 0, 20)
        valueDisplay.Position = UDim2.new(1, -70, 0, 0)
        valueDisplay.BackgroundTransparency = 1
        valueDisplay.TextColor3 = Config.Theme == "Dark" and Color3.new(1, 1, 1) or Color3.new(0, 0, 0)
        valueDisplay.Font = Enum.Font.SourceSans
        valueDisplay.TextSize = 16
        valueDisplay.Text = tostring(Config[configKey])
        valueDisplay.Parent = sliderFrame
        
        local sliderBg = Instance.new("Frame")
        sliderBg.Size = UDim2.new(1, -10, 0, 10)
        sliderBg.Position = UDim2.new(0, 5, 0, 25)
        sliderBg.BackgroundColor3 = Config.Theme == "Dark" and Color3.fromRGB(50, 50, 50) or Color3.fromRGB(150, 150, 150)
        sliderBg.BorderColor3 = Color3.fromRGB(100, 100, 100)
        sliderBg.Parent = sliderFrame
        
        local sliderFill = Instance.new("Frame")
        sliderFill.Size = UDim2.new((Config[configKey] - min) / (max - min), 0, 1, 0)
        sliderFill.BackgroundColor3 = Color3.fromRGB(0, 150, 255)
        sliderFill.BorderSizePixel = 0
        sliderFill.Parent = sliderBg
        
        local sliderButton = Instance.new("TextButton")
        sliderButton.Size = UDim2.new(0, 10, 0, 20)
        sliderButton.Position = UDim2.new((Config[configKey] - min) / (max - min), -5, 0, -5)
        sliderButton.BackgroundColor3 = Color3.new(1, 1, 1)
        sliderButton.BorderSizePixel = 0
        sliderButton.Text = ""
        sliderButton.Parent = sliderBg
        
        local dragging = false
        sliderButton.MouseButton1Down:Connect(function() dragging = true end)
        UserInputService.InputEnded:Connect(function(input) 
            if input.UserInputType == Enum.UserInputType.MouseButton1 then dragging = false end 
        end)
        UserInputService.InputChanged:Connect(function(input)
            if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
                local position = math.clamp((input.Position.X - sliderBg.AbsolutePosition.X) / sliderBg.AbsoluteSize.X, 0, 1)
                local value = min + (position * (max - min))
                value = math.floor(value / increment + 0.5) * increment
                Config[configKey] = value
                sliderFill.Size = UDim2.new(position, 0, 1, 0)
                sliderButton.Position = UDim2.new(position, -5, 0, -5)
                valueDisplay.Text = string.format("%.1f", value)
                nameLabel.Text = name .. ": " .. string.format("%.1f", value)
            end
        end)
        
        yPos = yPos + 55
    end
    
    addToggle("Show Highlights", "HighlightEnabled")
    addToggle("Show Names", "ShowName")
    addToggle("Show Health", "ShowHealth")
    addToggle("Show Distance", "ShowDistance")
    addToggle("Show Team", "ShowTeam")
    addToggle("Show Boxes", "BoxesEnabled")
    addToggle("Show Tracers", "TracersEnabled")
    addToggle("Show Chams", "ChamsEnabled")
    addToggle("Show Weapons", "ShowWeapons")
    addToggle("Show Armor Status", "ShowArmorStatus")
    addToggle("Show Skeletons", "ShowSkeletons")
    addToggle("Hide Teammates", "HideTeammates")
    addToggle("Rainbow Mode", "RainbowMode")
    addToggle("Headshot Mode", "HeadshotMode")
    addToggle("Team Check", "TeamCheckEnabled")
    addToggle("Radar Enabled", "RadarEnabled")
    addToggle("AI Detection", "AIDetection")
    addToggle("Voice Indicator", "VoiceIndicator")
    addToggle("Alert Mode", "AlertMode")
    addToggle("Wallhack", "Wallhack")
    addToggle("X-Ray Mode", "XRayMode")
    addToggle("Prediction Lines", "PredictionLines")
    addToggle("ESP Fade", "ESPFade")
    addToggle("Auto Adjust Distance", "AutoAdjustDistance")
    addToggle("Performance Mode", "PerformanceMode")
    addToggle("Custom Crosshair", "CustomCrosshair")
    addToggle("Loot ESP", "LootESP")
    addToggle("Vehicle ESP", "VehicleESP")
    addToggle("Silent Aim Indicator", "SilentAimIndicator")
    addToggle("FOV Circle", "FOVCircle")
    addToggle("Hitmarkers", "Hitmarkers")
    addToggle("Kill Effect", "KillEffect")
    addToggle("Sound ESP", "SoundESP")
    addToggle("Anti-AFK", "AntiAFK")
    addToggle("Auto Save Config", "AutoSaveConfig")
    addToggle("Role-Based Coloring", "ColorByRole")
    addToggle("Aimbot Enabled", "AimbotEnabled")
    
    addSlider("Max Distance", "MaxDistance", 100, 2000, 100)
    addSlider("Text Size", "TextSize", 10, 24, 1)
    addSlider("Box Thickness", "BoxThickness", 1, 5, 1)
    addSlider("ESP Thickness", "ESPThickness", 1, 5, 1)
    addSlider("Radar Size", "RadarSize", 50, 300, 10)
    addSlider("Radar Zoom", "RadarZoom", 0.5, 3, 0.1)
    addSlider("Alert Distance", "AlertDistance", 10, 100, 5)
    addSlider("Rainbow Speed", "RainbowSpeed", 1, 5, 0.5)
    addSlider("Crosshair Size", "CrosshairSize", 10, 50, 1)
    addSlider("Crosshair Thickness", "CrosshairThickness", 1, 5, 1)
    addSlider("FOV Radius", "FOVRadius", 50, 300, 10)
    addSlider("Radar Dot Size", "RadarDotSize", 2, 10, 1)
    addSlider("Update Frequency", "UpdateFrequency", 0.05, 0.5, 0.05)
    addSlider("Aimbot FOV", "AimbotFOV", 10, 100, 5)
    addSlider("Aimbot Smoothing", "AimbotSmoothing", 0.05, 0.5, 0.05)
    
    local themeButton = Instance.new("TextButton")
    themeButton.Size = UDim2.new(0, 100, 0, 30)
    themeButton.Position = UDim2.new(0, 5, 0, yPos)
    themeButton.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
    themeButton.Text = "Toggle Theme: " .. Config.Theme
    themeButton.TextColor3 = Color3.new(1, 1, 1)
    themeButton.Parent = scrollFrame
    themeButton.MouseButton1Click:Connect(function()
        Config.Theme = Config.Theme == "Dark" and "Light" or "Dark"
        mainFrame.BackgroundColor3 = Config.Theme == "Dark" and Color3.fromRGB(30, 30, 30) or Color3.fromRGB(200, 200, 200)
        mainFrame.BorderColor3 = Config.Theme == "Dark" and Color3.fromRGB(50, 50, 50) or Color3.fromRGB(100, 100, 100)
        titleLabel.BackgroundColor3 = Config.Theme == "Dark" and Color3.fromRGB(40, 40, 40) or Color3.fromRGB(180, 180, 180)
        titleLabel.TextColor3 = Config.Theme == "Dark" and Color3.new(1, 1, 1) or Color3.new(0, 0, 0)
        themeButton.Text = "Toggle Theme: " .. Config.Theme
        if self.Radar then
            self.Radar.Frame.BackgroundColor3 = Config.Theme == "Dark" and Color3.fromRGB(0, 0, 0) or Color3.fromRGB(200, 200, 200)
        end
    end)
    
    yPos = yPos + 35
    scrollFrame.CanvasSize = UDim2.new(0, 0, 0, yPos + 10)
    
    self.ConfigGui = {ScreenGui = configGui, MainFrame = mainFrame}
end

function ESP:Toggle()
    Config.Enabled = not Config.Enabled
    for _, espData in pairs(self.Objects) do
        espData.BillboardGui.Enabled = Config.Enabled
        espData.Highlight.Enabled = Config.Enabled and Config.HighlightEnabled
    end
    if self.Radar then self.Radar.Frame.Visible = Config.Enabled and Config.RadarEnabled end
    if self.Crosshair then self.Crosshair.Gui.Enabled = Config.Enabled and Config.CustomCrosshair end
    if self.FOVCircle then self.FOVCircle.Gui.Enabled = Config.Enabled and Config.FOVCircle end
end

function ESP:Cleanup()
    for _, connection in pairs(self.Connections) do connection:Disconnect() end
    for _, connection in pairs(self.CharacterConnections) do connection:Disconnect() end
    if self.Radar and self.Radar.UpdateConnection then self.Radar.UpdateConnection:Disconnect() end
    if self.AlertConnection then self.AlertConnection:Disconnect() end
    if self.InputConnection then self.InputConnection:Disconnect() end
    if ESPContainer then ESPContainer:Destroy() end
    if self.Radar and self.Radar.ScreenGui then self.Radar.ScreenGui:Destroy() end
    if self.ConfigGui and self.ConfigGui.ScreenGui then self.ConfigGui.ScreenGui:Destroy() end
    if self.AlertSystem then
        self.AlertSystem.Gui:Destroy()
        self.AlertSystem.Sound:Destroy()
    end
    if self.Crosshair then self.Crosshair.Gui:Destroy() end
    if self.FOVCircle then self.FOVCircle.Gui:Destroy() end
    for _, soundData in pairs(self.SoundSources) do soundData.Gui:Destroy() end
    self.Objects = {}
    self.Connections = {}
    self.CharacterConnections = {}
    self.LootObjects = {}
    self.VehicleObjects = {}
    self.SoundSources = {}
    self.Hitmarkers = {}
    self.LastUpdates = {}
end

ESP:Initialize()

_G.ToggleESP = function() ESP:Toggle() end
_G.CleanupESP = function() ESP:Cleanup() end

return ESP
