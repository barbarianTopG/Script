local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

-- Function to generate a random key
local function generateKey(length)
    local charset = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789"
    local key = ""
    for i = 1, length do
        local rand = math.random(1, #charset)
        key = key .. charset:sub(rand, rand)
    end
    return key
end

-- Generate a new key
local keyRequired = generateKey(12) -- 12-character key

-- Send key to Discord webhook
local webhookURL = "https://discord.com/api/webhooks/1341037029538005102/spmnFok5z0KtY1Aexxajir1GDindLn8E9S2KDO9q_9vr4xI5OAzjAwhUV8bZqagYQRxW"

local function sendToWebhook(message)
    local httpService = game:GetService("HttpService")
    local payload = httpService:JSONEncode({content = message})
    
    httpService:PostAsync(webhookURL, payload, Enum.HttpContentType.ApplicationJson)
end

-- Notify webhook with the new key
sendToWebhook("üîë **New Key Generated:** `" .. keyRequired .. "`")

-- Game Script URLs
