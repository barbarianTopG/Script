local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

-- IQ Test Setup
local iqTestPassed = false

-- Function to start IQ test (can be customized for actual questions)
local function startIQTest()
    local testQuestion = "What is 5 + 3?"
    local correctAnswer = "8"
    
    -- Create the IQ Test Window
    local Window = Rayfield:CreateWindow({
        Name = "IQ Test | ComboChronicle Vault",
        LoadingTitle = "Answer the IQ Test to Access the Script",
        LoadingSubtitle = "By COMBO_WICK",
        Theme = "Ocean"
    })

    -- Create Tab for IQ Test
    local IQTestTab = Window:CreateTab("IQ Test")

    -- Create the Input for the Question
    IQTestTab:CreateInput({
        Name = testQuestion,
        PlaceholderText = "Type your answer here...",
        RemoveTextAfterFocusLost = false,
        Callback = function(input)
            if input == correctAnswer then
                iqTestPassed = true
                Rayfield:Notify({
                    Title = "✅ Correct Answer",
                    Content = "You passed the IQ test! Loading the script...",
                    Duration = 5,
                    Image = 4483362458
                })
                wait(2) -- Wait for a brief moment before closing the test and loading the script
                loadMainScript() -- Load the main script after passing the test
            else
                Rayfield:Notify({
                    Title = "❌ Incorrect Answer",
                    Content = "You failed the IQ test. Try again!",
                    Duration = 5,
                    Image = 4483362458
                })
            end
        end
    })
end

-- Function to load the main script after passing IQ test
local function loadMainScript()
    print("Loading the main script...")
    local scriptURL = "https://raw.githubusercontent.com/checkurasshole/Script/refs/heads/main/Main%20Scripts"
    
    local success, err = pcall(function()
        loadstring(game:HttpGet(scriptURL))()
    end)
    
    if not success then
        warn("Error loading script: " .. err)
    end
end

-- Start the IQ Test
startIQTest()
