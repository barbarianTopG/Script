            continue
        end;
        local v78 = v77:FindFirstChild('HumanoidRootPart')
        if (not v78 or not v12.Character or not v12.Character:FindFirstChild('HumanoidRootPart')) then
            continue
        end;
        local v79 = (v78.Position - v12.Character.HumanoidRootPart.Position).Magnitude;
        if (v79 < v41) then
            v40 = v77;
            v41 = v79
        end
    end;
